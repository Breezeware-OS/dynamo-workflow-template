{
  "name": "dynamo-health-careteam-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^4.6.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/styles": "^5.14.18",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "aws-amplify": "^5.2.1",
    "axios": "^1.6.2",
    "env-cmd": "^10.1.0",
    "formik": "^2.4.5",
    "glide-design-system": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "rsuite": "^5.46.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2",
    "keycloak-js": "^25.0.1"

  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "npm test -- --coverage --watchAll --collectCoverageFrom='src/**/*.{test.js,test.jsx}'",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,css,scss}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/context-zone": "^1.17.1",
    "@opentelemetry/exporter-collector": "^0.25.1-alpha.23",
    "@opentelemetry/exporter-trace-otlp-http": "^0.41.2",
    "@opentelemetry/instrumentation-fetch": "^0.41.2",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "msw": "^1.3.1",
    "prettier": "^2.8.3",
    "standardx": "^7.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "babel": {},
  "standard": {
    "parser": "@babel/eslint-parser"
  }
}
